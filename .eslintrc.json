{
	"parser": "typescript-eslint-parser",
	"parserOptions": {
		"ecmaVersion": 3
	},
	"env": {
		"browser": true
	},
	"plugins": [
		"compat",
		"typescript"
	],
	"extends": "eslint:recommended",
	"rules": {
		"compat/compat": "warn",
		"no-warning-comments": "warn",
		"strict": ["error", "global"],
		"typescript/class-name-casing": "error",
		"typescript/explicit-function-return-type": "error",
		"typescript/explicit-member-accessibility": "error",
		"typescript/member-delimiter-style": "error",
		"typescript/member-ordering": "error",
		"typescript/no-angle-bracket-type-assertion": "error",
		"typescript/no-array-constructor": "error",
		"typescript/no-empty-interface": "error",
		"typescript/no-explicit-any": "error",
		"typescript/no-inferrable-types": "error",
		"typescript/no-unused-vars": "error",
		"typescript/type-annotation-spacing": "error"
	},
	"globals": {
		"COMPETENCES": false,
		"CONFIG": false,
		"FIELDS": false,
		"GROUPS": false,
		"LOGINSTATE": false,
		"Action": false,
		"ActionQueue": false,
		"ActionQueueSetup": false,
		"addCompetence": false,
		"addField": false,
		"addGroup": false,
		"addImage": false,
		"addLessonInFieldOnClick": false,
		"addOnClicks": false,
		"AfterLoadEvent": false,
		"authFailHandler": false,
		"changeCompetenceOnClick": false,
		"changeFieldOnClick": false,
		"changeGroupOnClick": false,
		"changeLessonCompetencesOnClick": false,
		"changeLessonFieldOnClick": false,
		"changeLessonGroupsOnClick": false,
		"changeUserGroupsOnClick": false,
		"changeUserRoleOnClick": false,
		"defaultBody": false,
		"defaultName": false,
		"deleteCompetenceOnClick": false,
		"deleteFieldOnClick": false,
		"deleteGroupOnClick": false,
		"deleteImageOnClick": false,
		"deleteLessonOnClick": false,
		"dialog": false,
		"dismissSpinner": false,
		"EasyMDE": false,
		"editor": false,
		"filterXSS": false,
		"getAttribute": false,
		"getLessonById": false,
		"historySetup": false,
		"imageSelectorOpen": false,
		"importGroupOnClick": false,
		"lessonHistoryOpen": false,
		"lessonSettings": false,
		"lessonSettingsCache": false,
		"lessonSettingsCacheEvent": false,
		"mainPageTab": false,
		"metadataEvent": false,
		"metadataSetup": false,
		"parseBoolForm": false,
		"parseVersion": false,
		"prepareImageSelector": false,
		"rawRequest": false,
		"reAuthHandler": false,
		"refreshLogin": false,
		"refreshMetadata": false,
		"refreshPreview": false,
		"refreshPreviewSetup": false,
		"renderPagination": false,
		"request": false,
		"restoreLesson": false,
		"showCompetenceSubview": false,
		"showdown": false,
		"showGroupSubview": false,
		"showImageSubview": false,
		"showLessonAddView": false,
		"showLessonEditor": false,
		"showLessonEditView": false,
		"showLessonRestoreView": false,
		"showLessonSubview": false,
