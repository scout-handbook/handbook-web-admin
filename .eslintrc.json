{
	"parserOptions": {
		"ecmaVersion": 3,
		"sourceType": "script"
	},
	"env": {
		"browser": true
	},
	"plugins": [
		"compat",
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"compat/compat": "warn",
		"no-warning-comments": "warn",
		"strict": ["error", "never"],
		"@typescript-eslint/array-type": ["error", "generic"],
		"@typescript-eslint/indent": ["error", "tab"],
		"@typescript-eslint/member-ordering": "error",
		"@typescript-eslint/no-extraneous-class": "error",
		"@typescript-eslint/no-namespace": ["error", {"allowDeclarations": true}],
		"@typescript-eslint/no-non-null-assertion": "off"
	},
	"globals": {
		"COMPETENCES": false,
		"CONFIG": false,
		"FIELDS": false,
		"GROUPS": false,
		"LOGINSTATE": false,
		"Action": false,
		"ActionQueue": false,
		"ActionQueueSetup": false,
		"addCompetence": false,
		"addField": false,
		"addGroup": false,
		"addImage": false,
		"addLessonInFieldOnClick": false,
		"addOnClicks": false,
		"AfterLoadEvent": false,
		"authFailHandler": false,
		"changeCompetenceOnClick": false,
		"changeFieldOnClick": false,
		"changeGroupOnClick": false,
		"changeLessonCompetencesOnClick": false,
		"changeLessonFieldOnClick": false,
		"changeLessonGroupsOnClick": false,
		"changeUserGroupsOnClick": false,
		"changeUserRoleOnClick": false,
		"defaultBody": false,
		"defaultName": false,
		"deleteCompetenceOnClick": false,
		"deleteFieldOnClick": false,
		"deleteGroupOnClick": false,
		"deleteImageOnClick": false,
		"deleteLessonOnClick": false,
		"deserializeAction": false,
		"dialog": false,
		"dismissSpinner": false,
		"EasyMDE": false,
		"editor": false,
		"filterXSS": false,
		"getAttribute": false,
		"getLessonById": false,
		"historySetup": false,
		"imageSelectorOpen": false,
		"importGroupOnClick": false,
		"lessonHistoryOpen": false,
		"lessonSettings": false,
		"lessonSettingsCache": false,
		"lessonSettingsCacheEvent": false,
		"mainPageTab": false,
		"metadataEvent": false,
		"metadataSetup": false,
		"parseBoolForm": false,
		"parseVersion": false,
		"prepareImageSelector": false,
		"rawRequest": false,
		"reAuthHandler": false,
		"refreshLogin": false,
		"refreshMetadata": false,
		"refreshPreview": false,
		"refreshPreviewSetup": false,
		"renderPagination": false,
		"request": false,
